{
  "name": "discord-ts-boilerplate",
  "version": "1.3.0",
  "description": "Discord bot built in TypeScript using discord.js",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "node scripts/build.js",
    "deploy": "node scripts/register_commands.js",
    "dev-deploy": "npm run build && npm run deploy",
    "clear-commands": "node scripts/register_commands.js --clear-commands",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts"
  },
  "keywords": [
    "discordbot",
    "discord.js",
    "typescript"
  ],
  "author": "Maximus7474",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^11.10.0",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.15.29",
    "eslint": "^9.28.0",
    "nodemon": "^3.1.10",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  }
}
